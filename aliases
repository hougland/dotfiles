# Elastic Search
alias es-start="~/elasticsearch-1.7.2/bin/elasticsearch -d"
alias es-stop="curl -X POST 'http://localhost:9200/_shutdown'"

# thefuck
eval "$(thefuck --alias)"

# ctags
alias ctags="`brew --prefix`/bin/ctags"

# correct emacs version
alias emacs="/usr/local/Cellar/emacs/24.x/Emacs.app/Contents/MacOS/Emacs -nw"
